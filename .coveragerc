[run]
branch = True
omit =
    flexfield/__init__.py
    flexfield/__main__.py
    flexfield/flexfield_cli.py

[report]
# Regexes for lines to exclude from consideration
exclude_lines =
    pragma: no cover
    def __repr__
    if self\.debug
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
